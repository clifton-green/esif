<section class="c-headed-text">
  <h1>{{ contentIntro.title }}</h1>
  {%if contentIntro.subtitle %}
  <h2>{{ contentIntro.subtitle }}</h2>
  {% endif %}
  <p>
    {{ contentIntro.bodyCopy }}
  </p>
  {%if contentIntro.ctaText != '' %}
    <a href="{{contentIntro.ctaURL}}" class="{{contentIntro.ctaType}}">{{contentIntro.ctaText}}</a>
  {% endif %}
</section>
